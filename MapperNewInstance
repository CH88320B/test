config.ForType<Encounters, SdocRoot>()
    .Map(dest => dest.OrderManagers, src =>
        src.OrderManagers != null && src.OrderManagers.Any()
            ? src.OrderManagers.Select(orderManager => new OrderManager
                {
                    OrderStatus = orderManager.OrderStatus,
                    // Otras propiedades...
                }).ToList()
            : new List<OrderManager>())
    .IgnoreNonMapped(true);
