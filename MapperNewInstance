config.ForType<Encounters, SdocRoot>()
    .Map(dest => dest.OrderManagers, src => src.OrderManagers != null && src.OrderManagers.Any()
        ? new List<OrderManager>
          {
              new OrderManager
              {
                  OrderStatus = src.OrderManagers.FirstOrDefault()?.OrderStatus
                  // Otras propiedades de OrderManager que puedas necesitar
              }
          }
        : null)
    .IgnoreNonMapped(true);
